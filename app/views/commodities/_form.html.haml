= form_for @commodity do |f|
  - if @commodity.errors.any?
    #error_explanation
      %h2= "#{pluralize(@commodity.errors.count, "error")} prohibited this commodity from being saved:"
      %ul
        - @commodity.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :id
    = f.number_field :id
  .field
    = f.label :product_id
    = f.number_field :product_id
  .field
    = f.label :time
    = f.text_field :time
  .field
    = f.label :price
    = f.text_field :price
  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
