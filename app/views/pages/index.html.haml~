#header
	= render :partial=>'layouts/header'
-if (defined?(@choice)).nil?
	-@choice = 1
#strip
	= render :partial=>'layouts/strip', :locals => {:frompage => "main" }
-comp = []
-namearray = []
-notearray = []
#content
	%span= 'choice ' + @choice.to_s
	
	-plist = Cheese.item[@choice.to_s]
	-psplit = plist.split(",")
	%span= 'plist ' + plist
	%span#leng.blank= psplit.length
