#strip		
	-if !(frompage == 'main')
		#stripmain
			= link_to "Main", index_path
	
	-if !(frompage == 'contact')
		#stripcontact
			= link_to "Contact", contact_path
	
	-if !(frompage == 'contact') 	
		#stripselect
